# PropsAI Backend Environment Variables (VPS)
# Copy this to backend.env and fill in your values

# App Settings
APP_ENV=production
APP_DEBUG=false
CORS_ORIGINS=http://YOUR_SERVER_IP

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-service-role-key

# SportsDataIO
SPORTSDATAIO_API_KEY=your-key

# OpenAI
OPENAI_API_KEY=your-key

# The Odds API
THE_ODDS_API_KEY=your-key
THE_ODDS_API_HISTORICAL_KEY=your-key

# BallDontLie API
BALLDONTLIE_API_KEY=your-key

# NewsAPI
NEWSAPI_KEY=your-key

# Twitter/X API
TWITTER_BEARER_TOKEN=your-token

# Tomorrow.io
TOMORROW_IO_API_KEY=your-key

# Visual Crossing
VISUAL_CROSSING_API_KEY=your-key

# Kalshi
KALSHI_API_KEY_ID=your-kalshi-key-id
KALSHI_PRIVATE_KEY_PATH=/app/kalshi.key

# Sentry
SENTRY_DSN=
